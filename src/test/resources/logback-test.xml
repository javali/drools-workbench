<configuration>
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <target>System.out</target>

        <encoder>
            <pattern>[%d{HH:mm:ss,SSS}] %p [%c: %L] %m%n</pattern>
        </encoder>
    </appender>

    <logger name="workflow" level="debug" />

    <root level="debug">
        <appender-ref ref="console" />
    </root>
</configuration>
